{
  "name": "mimmi20/macos-build",
  "type": "library",
  "description": "Class wrapper for peterkahl/apple-os-x-build-version",
  "homepage": "https://github.com/mimmi20/macos-build",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/macos-build/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18 || ~0.20.2",
    "mimmi20/coding-standard": "~1.0.9",
    "pepakriz/phpstan-exception-rules": "~0.11.6",
    "phpstan/extension-installer": "~1.1.0",
    "phpstan/phpstan": "~0.12.70",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.17",
    "phpunit/phpunit": "~7.5.20 || ~8.5.13 || ~9.5.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "MacosBuild\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MacosBuildTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/macos-build/issues",
    "source": "https://github.com/mimmi20/macos-build"
  }
}
